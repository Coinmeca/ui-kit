{
    "scope": "coinmeca-ui",
    "outputDirectory": ".next",
    "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}