{
    "name": "@coinmeca/ui",
    "version": "0.0.987363",
    "keywords": [
        "coinmeca",
        "interface",
        "ui"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/coinmeca/ethers"
    },
    "author": "Coinmeca Team <dev@coinmeca.net>",
    "license": "MIT",
    "sideEffects": false,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.esm.js",
    "files": [
        "./dist"
    ],
    "exports": {
        ".": {
            "require": "./dist/index.js"
        },
        "./*": [
            "./dist/*",
            "./dist/*.js",
            "./dist/*/index.js"
        ]
    },
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*",
                "./dist/*/index",
                "./dist/index"
            ]
        }
    },
    "scripts": {
        "dev": "npm pkg set type=commonjs && next dev",
        "build": "npm pkg set type=module && tsc --build && tsc-alias && tscp",
        "build:tsup": "npm pkg set type=module && tsup",
        "build:next": "npm pkg set type=commonjs && next build",
        "clean": "rimraf ./dist ./build ./.next ./.vercel ./.turbo ./tsconfig.tsbuildinfo ./node_modules/.cache",
        "start": "next start",
        "lint": "next lint",
        "deploy": "npm publish",
        "prebuild": "yarn clean"
    },
    "dependencies": {
        "@svgr/webpack": "^8.1.0",
        "chart.js": "^4.4.2",
        "framer-motion": "^11.0.8",
        "lightweight-charts": "^4.1.3",
        "sass": "^1.71.1",
        "sharp": "^0.33.2",
        "styled-components": "^6.1.8"
    },
    "peerDependencies": {
        "next": "^14.1.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/node": "^20.11.25",
        "@types/react": "^18.2.64",
        "@types/react-dom": "^18.2.21",
        "@vercel/analytics": "^1.2.2",
        "eslint": "^8.57.0",
        "eslint-config-next": "^14.1.3",
        "path-browserify": "^1.0.1",
        "tsc-alias": "^1.8.8",
        "tsup": "^8.0.2",
        "typescript": "^5.4.2",
        "typescript-cp": "^0.1.9",
        "vercel": "^33.5.5"
    },
    "packageManager": "yarn@4.2.2"
}