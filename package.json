{
    "name": "@coinmeca/ui",
    "version": "0.0.3",
    "keywords": [
        "coinmeca",
        "interface",
        "ui"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Coinmeca/ethers"
    },
    "author": "Coinmeca Team <dev@coinmeca.net>",
    "license": "MIT",
    "sideEffects": false,
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.esm.js",
    "files": [
        "dist"
    ],
    "exports": {
        ".": "./dist/index.js",
        "./*": "./dist/*"
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/*",
                "dist/*/index"
            ]
        }
    },
    "scripts": {
        "dev": "npm pkg set type=commonjs && next dev",
        "build": "npm pkg set type=module && tsc && tsc-alias && tscp",
        "build:tsup": "npm pkg set type=module && tsup",
        "build:next": "npm pkg set type=commonjs && next build",
        "clean": "rimraf ./dist ./build ./.next ./.vercel ./.turbo ./tsconfig.tsbuildinfo ./node_modules/.cache",
        "start": "next start",
        "lint": "next lint",
        "prebuild": "yarn clean"
    },
    "devDependencies": {
        "@svgr/webpack": "^8.1.0",
        "@types/node": "20.8.10",
        "@types/react": "18.2.36",
        "@types/react-dom": "18.2.14",
        "eslint": "8.53.0",
        "eslint-config-next": "14.0.1",
        "eslint-plugin-jsdoc": "^46.4.6",
        "path-browserify": "^1.0.1",
        "tsc-alias": "^1.8.8",
        "tsup": "^7.1.0",
        "typescript": "5.2.2",
        "typescript-cp": "^0.1.9",
        "vercel": "^32.5.0"
    },
    "dependencies": {
        "chart.js": "^4.4.0",
        "framer-motion": "^10.16.4",
        "lightweight-charts": "^4.1.1",
        "next": "14.0.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "sass": "^1.69.5",
        "shape": "^0.0.1",
        "styled-components": "^6.1.0"
    }
}
