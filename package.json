{
    "name": "@coinmeca/ui",
    "version": "0.0.1",
    "keywords": [
        "coinmeca",
        "smart-contract",
        "solidity",
        "ethereum",
        "ethers",
        "ether",
        "helper",
        "utility",
        "eip",
        "2535",
        "eip2535",
        "eip-2535",
        "diamond",
        "diamond pattern"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Coinmeca/ethers"
    },
    "author": "Coinmeca Team <dev@coinmeca.net>",
    "license": "MIT",
    "sideEffects": false,
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.esm.js",
    "files": [
        "dist"
    ],
    "exports": {
        ".": "./dist/index.js",
        "./*": "./dist/*"
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/*",
                "dist/*/index"
            ]
        }
    },
    "private": true,
    "scripts": {
        "dev": "npm pkg set type=commonjs && next dev",
        "build": "npm pkg set type=module && tsc && tsc-alias && tscp",
        "build:tsup": "npm pkg set type=module && tsup",
        "build:next": "npm pkg set type=commonjs && next build",
        "clean": "rimraf ./dist ./build ./.next ./.vercel ./.turbo ./tsconfig.tsbuildinfo ./node_modules/.cache",
        "start": "next start",
        "lint": "next lint",
        "prebuild": "yarn clean"
    },
    "dependencies": {
        "chart.js": "^4.3.2",
        "framer-motion": "^10.13.0",
        "lightweight-charts": "^4.0.1",
        "next": "13.4.12",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "sass": "^1.63.6",
        "shape": "^0.0.1",
        "styled-components": "^6.0.3"
    },
    "devDependencies": {
        "@svgr/webpack": "^8.0.1",
        "@types/node": "20.4.5",
        "@types/react": "18.2.18",
        "@types/react-dom": "18.2.7",
        "eslint": "8.46.0",
        "eslint-config-next": "13.4.12",
        "eslint-plugin-jsdoc": "^46.4.6",
        "path-browserify": "^1.0.1",
        "tsc-alias": "^1.8.7",
        "tsup": "^7.1.0",
        "typescript": "5.1.6",
        "typescript-cp": "^0.1.9",
        "vercel": "^31.2.2"
    }
}
